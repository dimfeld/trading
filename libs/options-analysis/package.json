{
  "name": "options-analysis",
  "version": "1.6.2",
  "description": "Utilities for analyzing option strategies",
  "main": "lib/index.js",
  "module": "lib-browser/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "uvu -r ts-node/register",
    "test:watch": "watchlist src -- rushx test",
    "clean": "rimraf types lib lib-browser",
    "build-types": "tsc --emitDeclarationOnly",
    "build-babel-base": "babel src/**/*.ts --extensions .ts --source-maps --ignore '**/*.spec.ts' --source-maps",
    "build-node": "BABEL_ENV=node rushx build-babel-base -d lib  ",
    "build-browser": "BABEL_ENV=browser rushx build-babel-base -d lib-browser",
    "watch-node": "yarn run build-node -w",
    "watch-browser": "yarn run build-browser -w",
    "watch-types": "yarn run build-types -w",
    "dev": "run-p watch-**",
    "build": "run-s clean && run-p build-node build-browser build-types",
    "add-lib": "git add lib; git add lib-browser; git add types"
  },
  "author": "Daniel Imfeld",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-modules": "^0.1.1",
    "@babel/preset-typescript": "^7.7.4",
    "@types/debug": "^0.0.30",
    "@types/lodash": "^4.14.76",
    "ts-node": "^9",
    "typescript": "^4",
    "rimraf": "~3.0.2",
    "npm-run-all": "~4.1.5",
    "uvu": "~0.3.3",
    "watchlist": "~0.2.3",
    "@types/node": "~14.10.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "hyperid": "^1.4.1",
    "lodash": "^4.17.4",
    "types": "~1.0.0",
    "npm-run-all": "~4.1.5",
    "sorters": "~0.0.3",
    "date-fns": "~2.16.1",
    "just-pick": "~2.1.0",
    "just-is-empty": "~2.0.1"
  }
}
